script/source = "extends CanvasLayer

@onready var unpause_button: Button = %unPauseButton

func _ready() -> void:
	unpause_button.pressed.connect(GameManager.toggle_pause)
	GameManager.pause.connect(toggle_show)
	
func toggle_show(is_paused : bool) -> void :
	if is_paused : show()
	else : hide()
"

